{
  "name": "postcss-js-styled",
  "version": "1.0.0",
  "description": "styled-components support for PostCSS and related tooling",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shenck215/postcss-js-styled.git"
  },
  "homepage": "https://github.com/shenck215/postcss-js-styled#readme",
  "bugs": {
    "url": "https://github.com/shenck215/postcss-js-styled/issues"
  },
  "main": "lib/main.js",
  "files": [
    "lib/!(test)",
    "docs"
  ],
  "scripts": {
    "clean": "rimraf ./lib",
    "lint": "eslint \"src/**/*.ts\"",
    "prebuild": "npm run clean",
    "build": "tsc",
    "test": "c8 --reporter=html --reporter=text-summary --reporter=lcov mocha \"lib/test/**/*_test.js\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test"
  },
  "author": "Shenck (https://github.com/shenck215)",
  "license": "MIT",
  "devDependencies": {
    "@types/babel__traverse": "^7.28.0",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "c8": "^7.12.0",
    "chai": "^4.3.7",
    "eslint": "^8.30.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^10.2.0",
    "postcss": "^8.4.20",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "postcss": "^8.3.11",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.6"
  },
  "dependencies": {
    "postcss-js-styled-core": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "postcss-less": {
      "optional": true
    },
    "postcss-scss": {
      "optional": true
    }
  }
}
